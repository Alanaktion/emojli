{
  "expo": {
    "name": "Emojli",
    "description": "A weird social network.",
    "slug": "emojli",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4dc0b5"
    },
    "primaryColor": "#4dc0b5",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.phpizza.emojli",
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.phpizza.emojli",
      "versionCode": 13,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-fg.png",
        "backgroundColor": "#4dc0b5"
      },
      "icon": "./assets/images/icon-android.png"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "./scripts/upload-sourcemaps",
          "config": {
            "organization": "alanaktion",
            "project": "emojli-mobile"
          }
        }
      ]
    },
    "extra": {
      "apiEndpoint": "https://emojli.phpizza.com/api",
      "sentryDsn": "https://1d8ee8dc455d46acac12fabbe18efd43@sentry.io/1417009"
    }
  }
}
