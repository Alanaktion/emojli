{
  "expo": {
    "name": "Emojli",
    "slug": "emojli",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4dc0b5"
    },
    "primaryColor": "#4dc0b5",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-fg.png",
        "backgroundColor": "#4dc0b5"
      },
      "icon": "./assets/images/icon-android.png"
    },
    "extra": {
      "apiEndpoint": "https://emojli.phpizza.com/api"
    }
  }
}
